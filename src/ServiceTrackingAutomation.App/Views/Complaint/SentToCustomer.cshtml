@model ServiceTrackingAutomation.Domain.Models.SentToCustomerModel

@{
    ViewData["Title"] = "SentToCustomer";
}


<h2>Şikayet Güncelle : Müşteriye Yollandı</h2>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="SentToCustomer">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="ComplaintId" class="control-label"></label>
                <input asp-for="ComplaintId" readonly class="form-control" />
                <span asp-validation-for="ComplaintId" class="text-danger"></span>
            </div>
            <br/>
            <div class="form-group">
                <label asp-for="CargoTrackingNumberToCustomer" class="control-label"></label>
                <input asp-for="CargoTrackingNumberToCustomer" class="form-control" />
                <span asp-validation-for="CargoTrackingNumberToCustomer" class="text-danger"></span>
            </div>
            <br/>

            <div class="form-group">
                <input type="submit" value="Kaydet" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
<br/>

<div>
    <a asp-action="List">Listeye Dön</a>
</div>

